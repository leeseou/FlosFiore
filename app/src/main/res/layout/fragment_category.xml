<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/category_main1_cclayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/category_main2_cclayout">

        <androidx.cardview.widget.CardView
            android:id="@+id/category_main1_cv"
            android:layout_width="156dp"
            android:layout_height="136dp"
            app:cardElevation="0dp"
            app:cardCornerRadius="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <ImageView
                android:id="@+id/category_main1_iv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/img_home_test"
                android:scaleType="centerCrop"/>
        </androidx.cardview.widget.CardView>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="18.28dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/ic_home_like_black"
            android:alpha="0.5"
            app:layout_constraintEnd_toEndOf="@id/category_main1_cv"
            app:layout_constraintBottom_toBottomOf="@id/category_main1_cv"/>

        <ImageView
            android:id="@+id/category_like1_iv"
            android:layout_width="20dp"
            android:layout_height="18.28dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/ic_home_like_white"
            app:layout_constraintEnd_toEndOf="@id/category_main1_cv"
            app:layout_constraintBottom_toBottomOf="@id/category_main1_cv"/>

        <LinearLayout
            android:id="@+id/category_price1_llayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/category_main1_cv"
            app:layout_constraintStart_toStartOf="parent"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/category_price1_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="88,000"
                android:textColor="@color/main_green1"
                android:textSize="14sp"
                android:fontFamily="@font/notosanscjkkr_bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="원"
                android:textColor="@color/main_green1"
                android:textSize="10sp"
                android:fontFamily="@font/notosanscjkkr_regular"/>
        </LinearLayout>

        <TextView
            android:id="@+id/category_name1_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="[플로레 화원] 가든 꽃바구니"
            android:textSize="10sp"
            android:textColor="@color/black"
            android:fontFamily="@font/notosanscjkkr_regular"
            app:layout_constraintTop_toBottomOf="@id/category_price1_llayout"
            app:layout_constraintStart_toStartOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
